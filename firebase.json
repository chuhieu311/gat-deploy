{
  "hosting": {
    "firebase": "gat-book-ver10",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/public/**",
        "function": "books"
      },
      {
        "source": "**",
        "function": "books"
      }
    ]
  }
}